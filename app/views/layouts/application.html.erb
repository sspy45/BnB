<!DOCTYPE html>
<html>
  <head>
    <%= csrf_meta_tags %>
    <%= javascript_include_tag "https://maps.googleapis.com/maps/api/js?key=AIzaSyCshnJi0tskQRZ_009mvGIguJdYxTO9uCg" %>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <link rel="stylesheet" href="path/to/font-awesome/css/font-awesome.min.css">
    <%= javascript_include_tag 'application' %>
    <script type="text/javascript">
      <% if signed_in? %>
        window.currentUser =
        <%= render(
          "api/partials/user.json.jbuilder",
          user: current_user
        ).html_safe %>
      <% end %>
    </script>
  </head>

  <body>
    <div id='root'></div>
  </body>
</html>
